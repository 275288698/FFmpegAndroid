<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kedacom.demo.appcameratoh264">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>

    <application
        tools:replace="android:icon"
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_h264"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity_EncoderController" android:screenOrientation="portrait">
        </activity>
        <activity android:name=".FirstActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PermissionActivity" android:screenOrientation="portrait" />


        <activity android:name=".InitActivity" android:screenOrientation="portrait"
            >

        </activity>

        <service android:name=".WorkService" />
    </application>

</manifest>